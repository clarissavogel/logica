programa
{
	
	funcao inicio()
	{
		real celsius

		escreva("Digite uma temperatura em graus Celsius: ")
		leia(celsius)

		escreva(celsius, " graus Celsius equivalem a ", ((celsius*1.8)+32), " graus Fhrenheit")
		
	}
}
